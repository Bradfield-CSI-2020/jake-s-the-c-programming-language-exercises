./expand a-z | wc -c | grep 27 && echo "success" || echo "fail"
./expand a-b-z | wc -c | grep 27 && echo "success" || echo "fail"
./expand A-z | wc -c | grep 4 && echo "success" || echo "fail"
./expand a-Z | wc -c | grep 4 && echo "success" || echo "fail"
./expand 0-9 | wc -c | grep 11 && echo "success" || echo "fail"
./expand 0-9- | wc -c | grep 12 && echo "success" || echo "fail"
./expand a-b-z- | wc -c | grep 28 && echo "success" || echo "fail"
./expand g-8 | wc -c | grep 4 && echo "success" || echo "fail"
./expand 00--00-0- | wc -c | grep 10 && echo "success" || echo "fail"
./expand m-j | wc -c | grep 4 && echo "success" || echo "fail"